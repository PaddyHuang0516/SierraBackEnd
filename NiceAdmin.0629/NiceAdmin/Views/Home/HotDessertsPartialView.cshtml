@model NiceAdmin.Models.ViewModels.DessertFrontIndexVM

<div class="col-sm-4 col-md-4">
    <div class="thumbnail">
        <img src="~/Uploads/@Model.DessertImageName" width="100" height="100" />

        <div class="caption">
            <h3>@Model.DessertName</h3>
            <p>售價: @Model.UnitPrice</p>
        </div>
    </div>
</div>